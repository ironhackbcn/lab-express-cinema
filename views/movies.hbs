<h2>Ironhack Cinema</h2>
<a href="/">Home</a>
<h3>Click on a movie to check the showtimes!</h3>
<div class="movie-list">
  {{#each movie}}
  <div class="movie-title">
    <img src="{{ this.image }}" />
    <h6>{{ this.title }}</h6>
    <a href="/movies/{{ this._id }}">See more</a>
  </div>
  {{/each}}
</div>