<h1>Ironhack Cinema</h1>
<h3>Click on a movie to check the showtimes!</h3>
<div class="movie-cards">
    {{#each movie}}
    <div class="movie-card">
        <img src="{{ this.image }}" />
        <h6>{{ this.title }}</h6>
        <a href="/movies/{{ this._id }}">See more</a>
    </div>
    {{/each}}
</div>