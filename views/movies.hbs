<div class="movies-container">
  <h1>Ironhack Cinema</h1>
  <p>Click on the movie to check the showtimes!</p>
  <ul class="movies-list-container">
    {{#each movies}}
    <li>
      <img src="{{this.image}}">
      <p>{{this.title}}</p>
      <a href="">See more</a>
    </li>
    {{/each}}
  </ul>
</div>