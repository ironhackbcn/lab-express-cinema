<div class="div-movies">
    <h2>Ironhack Cinema</h2>
    <p>Click on the movie to check the showtimes</p>
    <div class="div-images">
        {{#each movies}}
        <img src="{{this.image}}" alt="image">
        <h2>{{this.title}}</h2>
        <a href="/movie/{{this._id}}">See more</a>
        {{/each}}
    </div>

</div>

