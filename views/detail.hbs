<div class="detail-container">
  <img src="{{image}}">
  <div class="detail-info">
    <a href="">Go back</a>
    <h1>{{title}}</h1>
    <h2>Director: {{director}}</h2>
    <h3>Stars:</h3>
    <ul>
      {{#each stars}}
          <li>{{this}}</li>
      {{/each}}
    </ul>
    <p>{{description}}</p>
    <p> 
      {{#each showtimes}}
          {{this}} |
      {{/each}}
    </p>
  </div>
</div>

